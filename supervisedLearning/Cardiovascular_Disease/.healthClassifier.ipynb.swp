                "# Create BMI column\n",
                "JohnsBMI = dfEdit.ap_hi\n",
                "dfEdit['BMI'] = JohnsBMI\n",
                "\n",
                "# Populate 'BMI' column with categorical values\n",
                "for i in range(70000):\n",
                "     dfEdit.BMI[i] = round(dfEdit.weight[i]/(((dfEdit.height[i]) / 100) * ((dfEdit.height[i]) / 100)), 1)\n",
                "\n",
                "     if dfEdit.BMI[i] \u003c 18.5:\n",
                "          dfEdit.BMI[i] = 0 #Underweight/stick\n",
                "     elif dfEdit.BMI[i] \u003e= 18.5 and dfEdit.BMI[i] \u003c= 24.9:\n",
                "          dfEdit.BMI[i] = 1 #Normal/conformist\n",
                "     elif dfEdit.BMI[i] \u003e= 25.0 and dfEdit.BMI[i] \u003c= 29.9:\n",
                "          dfEdit.BMI[i] = 2 #Overweight/pudding\n",
                "     elif dfEdit.BMI[i] \u003e= 30 and dfEdit.BMI[i] \u003c= 34.9:\n",
