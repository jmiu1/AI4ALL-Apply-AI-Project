                "# Create BP column\n",
                "Johnsbp = dfEdit.age\n",
                "dfEdit['BP'] = Johnsbp\n",
                "\n",
                "# Populate 'BP' column with categorical values\n",
                "for i in range(70000):\n",
                "    \n",
                "    if dfEdit.ap_hi[i] \u003c 120 and dfEdit.ap_lo[i] \u003c 80:\n",
                "        dfEdit.BP[i] = 0 #Normal\n",
                "    elif dfEdit.ap_hi[i] \u003e= 120 and dfEdit.ap_hi[i] \u003c= 129 and dfEdit.ap_lo[i] \u003c 80:\n",
                "        dfEdit.BP[i] = 1 #Elevated\n",
